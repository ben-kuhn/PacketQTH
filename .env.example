# PacketQTH Environment Variables
# Copy this file to .env and customize

# HomeAssistant Long-Lived Access Token
# Generate in HA: Profile -> Long-Lived Access Tokens
HA_TOKEN=your_homeassistant_token_here

# Optional: Override log level
# Values: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Optional: Override HomeAssistant URL
# HA_URL=http://homeassistant.local:8123
